        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TwoFactorAuth"
        mc:Ignorable="d"
        Title="Код" Height="80" Width="200">
    <Grid>
        <GroupBox Header="Введите код" HorizontalAlignment="Center" VerticalAlignment="Center" Width="150">
            <TextBox Name="OneTimeCode" TextChanged="OneTimeCode_TextChanged"/>
        </GroupBox>
    </Grid>
</Window>
